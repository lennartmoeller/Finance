{
    "compilerOptions": {
        "target": "es6",
        "lib": ["dom", "es2022"],
        "jsx": "react",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": "./src",
        "paths": {
            "@/*": ["*"]
        }
    },
    "include": ["src/**/*"]
}
